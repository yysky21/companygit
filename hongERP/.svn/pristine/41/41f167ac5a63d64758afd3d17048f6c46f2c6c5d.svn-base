table {
    font-size: 12px;
}
.daterangepicker .calendar th, .daterangepicker .calendar td{
    min-width: 22px;
}

.h2-margin-bottom{
    margin-bottom: 20px;
}

#details input{
    background-color: snow;
    border: 1px solid #ccc;
    line-height: 22px;
    width:90px;
    border-radius: 3px;
}

#details div{
    padding-left: 10px;
    padding-bottom: 10px;
}

#details i{
    -moz-transform:scale(1.5,1.5);
    -webkit-transform:scale(1.5,1.5);
    -o-transform:scale(1.5,1.5);
}

.table-sheet {
    border: 0;
    padding: 0;
    white-space: nowrap;
}

.table-sheet > thead{
    background: rgba(52, 73, 94, 0.94);
    color: #ECF0F1;
}

.table-sheet > thead > tr > th{
    text-align: center;
    border-right: 1px solid #ddd;
    height: 35px;
}

.table-sheet > thead > tr > th:last-child{
    border-right: 0;
}


.table-sheet > tbody > tr > td{
    padding: 0;
    border: 1px solid #ddd;
}

.table-sheet > tbody > tr > td > input{
    background-color: snow;
    border: 0;
    border-radius: 0;
    -webkit-box-shadow: inset 0 0 0;
    box-shadow: inset 0 0 0;
}

.table-sheet > tbody > tr > td > select{
    width: 171px;
    background-color: snow;
    border: 0;
    border-radius: 0;
    -webkit-box-shadow: inset 0 0 0;
    box-shadow: 0;
}

.table-sheet > tbody > tr > td:first-child{
    border-left: 0;
}

.table-sheet > tbody > tr > td:last-child{
    border-right: 0;
}

.table-sheet > tbody > tr:last-child > td{
    border-bottom: 0;
}

#tbody > tr > td{
    height: 30px;
    text-align: center;
}

.product-property-input > tbody > tr > td > input {
    width: 200px;
}